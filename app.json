{
  "expo": {
    "name": "Yo App",
    "slug": "yo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "projectId": "yo-app-mobile",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yoapp.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366f1"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yoapp.mobile",
      "permissions": ["NOTIFICATIONS", "VIBRATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "API_URL": null
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#6366f1"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#6366f1",
          "sounds": ["./assets/sounds/yo-sound.wav"],
          "mode": "production"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceGrotesk-Regular.ttf",
            "./assets/fonts/SpaceGrotesk-Bold.ttf"
          ]
        }
      ]
    ]
  }
}
